  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "[docplex.mp.LinearConstraint[](inv_1_1,GE,0),\n",
       " docplex.mp.LinearConstraint[](inv_1_2,GE,0),\n",
       " docplex.mp.LinearConstraint[](inv_1_3,GE,0),\n",
