   "source": [
    "X = model.continuous_var_matrix(products, period, name = \"prod\") # production quantity\n",
    "Y = model.continuous_var_matrix(products, period, name = \"inv\") # inventory level\n",
    "S = model.binary_var_matrix(products, period, name = \"set\")#Binary variable of setup\n",
    "\n",
    "print(X)\n",
    "print(Y)\n",
    "print(S)"
   ]
