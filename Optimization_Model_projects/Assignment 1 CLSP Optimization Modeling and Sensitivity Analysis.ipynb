    "# Initialize a DataFrame to store the results of the sensitivity analysis\n",
    "header = ['inventory_holding_costs','Total_cost','Prod_t1','Prod_t2','Prod_t3','Prod_t4','Prod_t5','Prod_t6','Prod_t7','Prod_t8','Prod_t9','Prod_t10','Inventory_1','Inventory_2','Inventory_3','Inventory_4','Inventory_5','Inventory_6','Inventory_7','Inventory_8','Inventory_9','Inventory_10',\"setup_cost_1\",\"setup_cost_2\",\"setup_cost_3\",\"setup_cost_4\",\"setup_cost_5\",\"setup_cost_6\",\"setup_cost_7\",\"setup_cost_8\",\"setup_cost_9\",\"setup_cost_10\"]\n",
    "output = pd.DataFrame(columns = header)\n",
    "\n",
    "for inventory_holding_costs in range(1,21): # Loop over different values for I to analyze the sensitivity with respect to I\n",
    "\n",
    "    # Define linear expressions\n",
    "    Total_cost = model.sum(inventory_holding_costs * Y[k,t] + costs.loc[k,\"setup costs per setup\"]* S[k,t] for k in products for t in period)\n",
    "    \n",
    "    # Define the objective function \n",
    "    model.minimize(Total_cost)\n",
    "\n",
    "    # Solve the model\n",
    "    model.solve()\n",
    "    \n",
    "    # Store the value of I and the objecive function value in the output DataFrame\n",
    "    output.loc[inventory_holding_costs-1,'inventory_holding_costs'] = inventory_holding_costs\n",
    "    output.loc[inventory_holding_costs-1, 'Total_cost'] = model.objective_value * 100 # costs are expressed in hundreds of euro\n",
    "    \n",
    "    # Store the total production, overtime and inventory of all products per period in the output DataFrame\n",
    "    for t in period:\n",
    "        Xt = 0\n",
    "        Yt = 0\n",
    "        St = 0\n",
    "        for k in products:\n",
    "            Xt += X[k,t].solution_value\n",
    "            Yt += Y[k,t].solution_value\n",
    "            St += S[k,t].solution_value * 100\n",
    "        output.iloc[inventory_holding_costs-1, t+1] = round(Xt) \n",
    "        output.iloc[inventory_holding_costs-1, t+11] = round(Yt)\n",
    "        output.iloc[inventory_holding_costs-1, t+21] = round(St)\n",
    "    \n",
    "    \n",
    "print(output)\n",
    "\n"
   ]
