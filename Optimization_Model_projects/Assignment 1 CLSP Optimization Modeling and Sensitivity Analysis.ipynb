   ],
   "source": [
    "\n",
    "#Inventory Balance\n",
    "model.add_constraints(Y[k,t-1] + X[k,t] - demand.loc[k,t] == Y[k,t] for k in products for t in range(2,11))\n",
    "model.add_constraints(starting_inventories.loc[k,'initial inventory'] + X[k,1] - demand.loc[k,1] == Y[k,1] for k in products)\n",
    "\n",
    "#Linking Constraints\n",
    "model.add_constraints(X[k,t] <= production_parameters.loc[\"BigM\",\"value\"] * S[k,t] for k in products for t in period)\n",
    "\n",
    "#Capacity Constraints\n",
    "model.add_constraints(model.sum(production_coefficients.loc[k,\"production coefficient\"] * X[k,t] for k in products) <= production_parameters.loc[\"capacity per period\",\"value\"] for t in period)\n",
    "\n",
    "model.add_constraints(X[k,t] >= 0 for k in products for t in period)\n",
    "model.add_constraints(Y[k,t] >= 0 for k in products for t in period)\n",
    "#No consecutive production for P1\n",
    "model.add_constraints(S[1,t] + S[1,t-1] <= 1 for t in range(2,11))\n",
    "\n",
    "#Objective Function\n",
    "Z = model.sum(costs.loc[k,\"inventory costs per unit and period\"] * Y[k,t] + costs.loc[k,\"setup costs per setup\"]* S[k,t] for k in products for t in period)\n",
    "model.minimize(Z)\n",
    "\n",
    "#Solve model\n",
    "model.solve()\n",
    "model.solve_details\n",
    "\n",
    "#Print results\n",
    "header = ['Period','Production P1', 'Production P2']\n",
    "df = pd.DataFrame(columns = header)\n",
    "\n",
    "for t in period:\n",
    "    df.loc[len(df), 'Period'] = t\n",
    "    df.loc[len(df)-1, 'Production P1'] = X[1,t].solution_value\n",
    "    df.loc[len(df)-1, 'Production P2'] = X[2,t].solution_value\n",
    "print(df)\n",
    "print(\"\\n\")\n",
    "print(\"The objective value is \", model.objective_value) # costs are expressed in hundreds of euro!\n"
   ]
  }
