     "text": [
      "  Period Production P1 Production P2\n",
      "0      1            70            30\n",
      "1      2             0             0\n",
      "2      3            60             0\n",
      "3      4             0            85\n",
      "4      5            80             0\n",
      "5      6             0             0\n",
      "6      7             0             0\n",
      "7      8            90            35\n",
      "8      9             0             0\n",
